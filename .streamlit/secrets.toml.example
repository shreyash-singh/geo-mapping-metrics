# Streamlit Secrets Template
# Copy this file to secrets.toml and fill in your API keys
# OR configure secrets directly in Streamlit Cloud dashboard
#
# Note: secrets.toml is in .gitignore and will NOT be committed to the repository
# Each user must provide their own API keys

# Google Maps API Key (required for Geo Metrics)
GOOGLE_MAPS_API_KEY = "YOUR_GOOGLE_MAPS_API_KEY_HERE"

# Mapbox API Token (required for Geo Mapping)
MAPBOX_API_TOKEN = "YOUR_MAPBOX_API_TOKEN_HERE"
